{
  "name": "@sphericalelephant/exseq",
  "version": "2.5.2",
  "description": "Express based automatic REST generator for Sequelize.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test4": "NODE_ENV=test SEQUELIZE_VERSION=sequelize4 mocha",
    "test5": "NODE_ENV=test SEQUELIZE_VERSION=sequelize5 mocha",
    "test": "npm run test4 && npm run test5",
    "coverage": "NODE_ENV=test SEQUELIZE_VERSION=sequelize4,sequelize5 istanbul cover _mocha -- -R spec",
    "test-ci": "SEQUELIZE_VERSION=sequelize5 istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Patrick Sturm <p.sturm@sphericalelephant.com> (https://sphericalelephant.com/)",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/SphericalElephant/ExSeq.git"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "express": "^4.16.4",
    "lodash": "^4.17.14",
    "mime-types": "^2.1.18",
    "semver-regex": "^1.0.0"
  },
  "keywords": [
    "express",
    "sequelize",
    "rest",
    "autorest",
    "generator",
    "restful"
  ],
  "engines": {
    "node": ">=8.12"
  },
  "devDependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-loopback": "^13.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "rewire": "^4.0.1",
    "sequelize4": "npm:sequelize@^4.44.2",
    "sequelize5": "npm:sequelize@^5.10.1",
    "sqlite3": "^4.0.6",
    "supertest": "^3.4.2"
  }
}
